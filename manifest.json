{
  "manifest_version": 3,
  "name": "Web Icon Download 网站图标查看-快速下载网站图标",
  "version": "1.0",
  "description": "View icons of current browser tabs 查看下载网站图标 ",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
